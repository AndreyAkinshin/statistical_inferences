@article{wiese_later_2019,
  title = {Later but Not Early Stages of Familiar Face Recognition Depend Strongly on Attentional Resources: {{Evidence}} from Event-Related Brain Potentials},
  shorttitle = {Later but Not Early Stages of Familiar Face Recognition Depend Strongly on Attentional Resources},
