  journal = {Journal of Experimental Social Psychology},
  keywords = {Cheating,Costs,Dishonesty,OSBadgeOpenData,OSBadgeOpenMaterials,Punishment,Sanctions,Utility}
}

@article{thielmann_isabel_seeing_2017,
