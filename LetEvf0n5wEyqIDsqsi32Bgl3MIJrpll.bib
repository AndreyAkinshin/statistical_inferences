@article{critcher_moral_2020,
  title = {Moral Character Evaluation: {{Testing}} Another's Moral-Cognitive Machinery},
  shorttitle = {Moral Character Evaluation},
  author = {Critcher, Clayton R. and Helzer, Erik G. and Tannenbaum, David},
