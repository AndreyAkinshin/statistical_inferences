  file = {C\:\\Users\\dlakens\\Zotero\\storage\\TBXBUUMG\\jones_index.html},
  journal = {Internet Archaeology},
  keywords = {OSBadgeOpenData},
  language = {en},
  number = {19}
}

@article{jones_density_2020,
