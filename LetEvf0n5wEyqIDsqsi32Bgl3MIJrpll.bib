  journal = {Advances in Methods and Practices in Psychological Science},
  keywords = {Bayes factor analysis,NHST,nonsignificant results,open data,open materials,OSBadgeOpenData,OSBadgeOpenMaterials,OSBadgePreregistered,preregistered},
  language = {en},
  number = {3}
}

