  file = {C\:\\Users\\dlakens\\Zotero\\storage\\6TEJ4UY2\\Muramatsu et al. - 2019 - Novel regulatory systems for acetylcholine release.pdf;C\:\\Users\\dlakens\\Zotero\\storage\\5RXDLI3V\\jnc.html},
  journal = {Journal of Neurochemistry},
  keywords = {acetylcholine release,acetylcholine uptake,anti-Alzheimer's disease drugs,OSBadgeOpenMaterials,pre-synaptic muscarinic receptors,rat striatum,superfusion},
