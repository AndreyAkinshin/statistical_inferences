  shorttitle = {Connecting in the {{Twitterverse}}},
  author = {Iannone, Nicole E. and McCarty, Megan K. and Branch, Sara E. and Kelly, Janice R.},
  year = {2018},
  month = jul,
  volume = {158},
  pages = {491--495},
  issn = {0022-4545, 1940-1183},
