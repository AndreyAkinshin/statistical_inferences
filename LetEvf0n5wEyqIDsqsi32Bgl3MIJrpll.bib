  file = {C\:\\Users\\dlakens\\Zotero\\storage\\3LSSA8YL\\Oliveira et al. - 2019 - Crosstalk between mitochondria, calcium channels a.pdf;C\:\\Users\\dlakens\\Zotero\\storage\\NMBGSJTJ\\jnc.html},
  journal = {Journal of Neurochemistry},
  keywords = {actin cytoskeleton,calcium channels,locus coeruleus,mitochondria,neuronal activity,noradrenergic,OSBadgeOpenMaterials},
  language = {en},
  number = {4}
}

@article{olivola_interpersonal_2018,
