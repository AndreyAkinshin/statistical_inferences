@article{crossman_effects_2019,
  title = {Effects of Transcranial Direct Current Stimulation over the Posterior Parietal Cortex on Episodic Memory Reconsolidation},
  author = {Crossman, Margot and Bartl, Gergely and Soerum, Rebecca and Sandrini, Marco},
  year = {2019},
  month = dec,
  volume = {121},
  pages = {78--88},
  issn = {0010-9452},
  doi = {10.1016/j.cortex.2019.08.009},
  abstract = {Consolidated memories may return to labile/unstable states after their reactivation, thus requiring a restabilization process that is known as reconsolidation. During this time-limited reconsolidation window, reactivated existing memories can be strengthened, weakened or updated with new information. Previous studies have shown that non-invasive stimulation of the lateral prefrontal cortex after memory reactivation strengthened existing verbal episodic memories through reconsolidation, an effect documented by enhanced delayed memory recall (24~h post-reactivation). However, it remains unknown whether the left posterior parietal cortex (PPC), a region involved during reactivation of existing episodic memories, contributes to reconsolidation. To address this question, in this double-blind experiment healthy participants (n~=~27) received transcranial direct current stimulation (tDCS) with the anode over the left PPC after reactivation of previously learned verbal episodic memories. Memory recall was tested 24~h later. To rule out unspecific effects of memory reactivation or tDCS alone, we included two control groups: one that receives tDCS with the anode over the left PPC without reactivation (n~=~27) and another one that receives tDCS with the anode over a control site (primary visual cortex) after reactivation (n~=~27). We hypothesized that tDCS with the anode over the left PPC after memory reactivation would enhance delayed recall through reconsolidation relative to the two control groups. No significant between groups differences in the mean number of words recalled on day 3 occurred, suggesting no beneficial effect of tDCS over the left PPC. Alternative explanations were discussed, including efficacy of tDCS, different stimulation parameters, electrode montage, and stimulation site within the PPC.},
  file = {C\:\\Users\\dlakens\\Zotero\\storage\\CIUKFISM\\Crossman et al. - 2019 - Effects of transcranial direct current stimulation.pdf;C\:\\Users\\dlakens\\Zotero\\storage\\TEWXKADY\\S0010945219302977.html},
  journal = {Cortex},
  keywords = {Episodic memory,Long-term memory,OSBadgeOpenData,OSBadgeOpenMaterials,OSBadgePreregistered,Parietal cortex,Reconsolidation,tDCS},
  language = {en}
}

