  author = {Chernoff, Benjamin L. and Teghipco, Alex and Garcea, Frank E. and Belkhir, Raouf and Sims, Max H. and Paul, David A. and Tivarus, Madalina E. and Smith, Susan O. and Hintz, Eric and Pilcher, Webster H. and Mahon, Bradford Z.},
  year = {2020},
  month = feb,
  volume = {123},
