  copyright = {\textcopyright{} 2019 International Society for Neurochemistry},
  file = {C\:\\Users\\dlakens\\Zotero\\storage\\HQ5225YY\\Forsberg et al. - 2019 - Ionized calcium in human cerebrospinal fluid and i.pdf;C\:\\Users\\dlakens\\Zotero\\storage\\CENJ4TS6\\jnc.html},
  journal = {Journal of Neurochemistry},
