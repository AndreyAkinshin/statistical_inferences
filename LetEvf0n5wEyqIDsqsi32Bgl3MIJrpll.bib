  journal = {Journal of Experimental Social Psychology},
  keywords = {Dyadic models of health behavior,Health behavior,OSBadgeOpenData,OSBadgeOpenMaterials,OSBadgePreregistered,Parent-adolescent relationships,Parenting styles,Theory of Planned Behavior},
  series = {Health in {{Context}}: {{New Perspectives}} on {{Healthy Thinking}} and {{Healthy Living}}}
