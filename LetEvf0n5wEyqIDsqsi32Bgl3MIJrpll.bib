  file = {C\:\\Users\\dlakens\\Zotero\\storage\\9SE7VPSQ\\Vitriol et al. - 2019 - Ideological uncertainty and investment of the self.pdf;C\:\\Users\\dlakens\\Zotero\\storage\\XGC49Q8N\\S0022103118301732.html},
