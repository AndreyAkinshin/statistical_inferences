}

@article{zhou_analysis_2019,
  title = {Analysis of Long Non-Coding {{RNA}} Expression Profiles in Neonatal Rats with Hypoxic-Ischemic Brain Damage},
  author = {Zhou, Han and Wang, Xuan and Cheng, Rui and Hou, Xuewen and Chen, Ying and Feng, Yun and Qiu, Jie},
  year = {2019},
