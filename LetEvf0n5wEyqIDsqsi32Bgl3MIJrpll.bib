  journal = {Language Learning},
  keywords = {contextual inference,OSBadgeOpenData,OSBadgeOpenMaterials,paired-associate learning,retrieval,second language,testing effect,vocabulary learning},
  number = {2}
}

@article{van_der_burght_intonation_2019,
