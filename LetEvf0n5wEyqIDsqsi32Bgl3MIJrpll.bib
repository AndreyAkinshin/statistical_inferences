@article{fisher_are_2019,
  title = {Are Single People a Stigmatized 'Group'? {{Evidence}} from Examinations of Social Identity, Entitativity, and Perceived Responsibility},
  shorttitle = {Are Single People a Stigmatized 'Group'?},
