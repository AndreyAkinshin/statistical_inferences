  author = {Aczel, Balazs and Palfi, Bence and Szollosi, Aba and Kovacs, Marton and Szaszi, Barnabas and Szecsi, Peter and Zrubka, Mark and Gronau, Quentin F. and {van den Bergh}, Don and Wagenmakers, Eric-Jan},
