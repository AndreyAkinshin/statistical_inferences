@article{oberfeld_evaluating_2013,
  title = {Evaluating the Robustness of Repeated Measures Analyses: {{The}} Case of Small Sample Sizes and Nonnormal Data},
  shorttitle = {Evaluating the Robustness of Repeated Measures Analyses},
  author = {Oberfeld, Daniel and Franke, Thomas},
