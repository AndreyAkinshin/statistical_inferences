@article{riesel_overactive_2015,
  title = {Overactive {{Performance Monitoring}} as an {{Endophenotype}} for {{Obsessive}}-{{Compulsive Disorder}}: {{Evidence From}} a {{Treatment Study}}},
  shorttitle = {Overactive {{Performance Monitoring}} as an {{Endophenotype}} for {{Obsessive}}-{{Compulsive Disorder}}},
