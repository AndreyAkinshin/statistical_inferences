@article{bratton_critical_2005,
  title = {Critical {{Mass Theory Revisited}}: {{The Behavior}} and {{Success}} of {{Token Women}} in {{State Legislatures}}},
  shorttitle = {Critical {{Mass Theory Revisited}}},
  author = {Bratton, Kathleen A.},
  year = {2005},
  month = mar,
  volume = {1},
