@article{baldissera_differential_1991,
  title = {Differential Control of In-Phase and Anti-Phase Coupling of Rhythmic Movements of Ipsilateral Hand and Foot},
  author = {Baldissera, F. and Cavallari, P. and Marini, G. and Tassone, G.},
  year = {1991},
