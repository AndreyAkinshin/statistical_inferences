@article{cribbie_testing_2016,
  title = {Testing for Negligible Interaction: {{A}} Coherent and Robust Approach},
  shorttitle = {Testing for Negligible Interaction},
  author = {Cribbie, Robert A. and Ragoonanan, Chantal and Counsell, Alyssa},
  year = {2016},
  month = may,
  volume = {69},
  pages = {159--174},
  issn = {2044-8317},
  doi = {10.1111/bmsp.12066},
  abstract = {Researchers often want to demonstrate a lack of interaction between two categorical predictors on an outcome. To justify a lack of interaction, researchers typically accept the null hypothesis of no interaction from a conventional analysis of variance (ANOVA). This method is inappropriate as failure to reject the null hypothesis does not provide statistical evidence to support a lack of interaction. This study proposes a bootstrap-based intersection\textendash union test for negligible interaction that provides coherent decisions between the omnibus test and post hoc interaction contrast tests and is robust to violations of the normality and variance homogeneity assumptions. Further, a multiple comparison strategy for testing interaction contrasts following a non-significant omnibus test is proposed. Our simulation study compared the Type I error control, omnibus power and per-contrast power of the proposed approach to the non-centrality-based negligible interaction test of Cheng and Shao (2007, Statistica Sinica, 17, 1441). For 2~\texttimes ~2 designs, the empirical Type I error rates of the Cheng and Shao test were very close to the nominal {$\alpha$} level when the normality and variance homogeneity assumptions were satisfied; however, only our proposed bootstrapping approach was satisfactory under non-normality and/or variance heterogeneity. In general a~\texttimes ~b designs, although the omnibus Cheng and Shao test, as expected, is the most powerful, it is not robust to assumption violation and results in incoherent omnibus and interaction contrast decisions that are not possible with the intersection\textendash union approach.},
  file = {C\:\\Users\\dlakens\\Zotero\\storage\\PS872PDJ\\abstract.html},
  journal = {British Journal of Mathematical and Statistical Psychology},
  keywords = {equivalence testing,factorial ANOVA,lack of interaction,negligible interaction},
  language = {en},
  number = {2}
}

