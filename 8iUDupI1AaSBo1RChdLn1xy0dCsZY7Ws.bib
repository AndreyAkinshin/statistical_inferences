@article{schroder_affective_2013,
  title = {The Affective Meanings of Automatic Social Behaviors: {{Three}} Mechanisms That Explain Priming.},
  shorttitle = {The Affective Meanings of Automatic Social Behaviors},
  author = {Schr{\"o}der, Tobias and Thagard, Paul},
