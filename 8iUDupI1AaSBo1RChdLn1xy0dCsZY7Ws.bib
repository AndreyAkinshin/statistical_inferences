@article{weston_recommendations_2018,
  title = {Recommendations for Increasing the Transparency of Analysis of Pre-Existing Datasets},
  author = {Weston, Sara J. and Ritchie, Stuart J. and Rohrer, Julia M. and Przybylski, Andrew K.},
  year = {2018},
  month = jul,
  doi = {10/gfpqck},
  abstract = {Important strides have been made in recent years to promote increasing the reliability and transparency of scientific practice, including the increased availability of open data, and open materials. A major development is the use of preregistered sampling and analysis plans, which enable readers to distinguish between analyses meant to generate versus test theories.. However, these practices have tended to be most immediately applicable to experimental researchers and those who collect new data for each new study. Those whose research involves secondary analysis\textemdash modeling and predicting phenomena using pre-existing datasets\textemdash often believe they are unable to engage in many of these new practices. For example, because the data may have existed for many years, full preregistration is not possible. Nonetheless, there are many ways in which secondary analysts can increase the openness and transparency of their research. Here, we provide nine recommendations for integrating open science practices into analysis of pre-existing data, including being more open about previous work published on the same dataset and registering an analysis plan prior to statistical modeling. These recommendations are expected to be facilitated by a forthcoming registration template on the Open Science Framework. We end by anticipating potential objections to our recommendations.},
  file = {C\:\\Users\\dlakens\\Zotero\\storage\\MBH6UCIW\\Weston et al. - 2018 - Recommendations for increasing the transparency of.pdf;C\:\\Users\\dlakens\\Zotero\\storage\\LQFIL7CL\\zmt3q.html}
